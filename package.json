{
  "name": "data-intake",
  "version": "1.0.0",
  "description": "Serviço de Extração e Chunking de Documentos com LangChain, OpenAI e ChromaDB",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint src test --fix",
    "lint:validate": "eslint src test",
    "test": "npm run clean && jest --passWithNoTests --config ./test/jest-e2e.json",
    "test:unit": "jest --config ./jest.config.js",
    "test:unit:watch": "jest --config ./jest.config.js --watch",
    "test:unit:cov": "jest --config ./jest.config.js --coverage",
    "test:retry": "jest --config ./jest.config.js test/modules/common/retry.decorator.spec.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --passWithNoTests --config ./test/jest-e2e.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --passWithNoTests --config ./test/jest-e2e.json",
    "extraction:example": "tsx scripts/extraction-example.ts",
    "extraction:test": "npm run test:unit -- src/modules/extraction"
  },
  "keywords": [
    "nestjs",
    "langchain",
    "openai",
    "chromadb",
    "extraction",
    "chunking",
    "embeddings",
    "vector-store",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.0.57",
    "@langchain/openai": "^0.5.16",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.6.2",
    "chromadb": "^1.10.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "langchain": "^0.1.37",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^4.47.1",
    "oracledb": "^6.3.0",
    "pdf-parse": "^1.1.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sync-request": "^6.1.0",
    "typeorm": "^0.3.25",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "yaml": "^2.8.0",
    "@azure/identity": "^4.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsx": "^4.7.3",
    "typescript": "^5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
